    [] dummyFirst & v14 = unspaceStatus & throughQueue < maxQueueSize -> 1/8 : (dPos' = 1) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 2) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 3) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 4) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 5) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 6) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 7) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false) + 1/8 : (dPos' = 8) & (v14' = spaceStatus) & (throughQueue' = throughQueue + 1) & (dummyFirst' = false);

    [] v14 = spaceStatus & dPos = 1 & (v0 = crossingStatus | v0 = stoppedStatus | v0 = crossedStatus) & throughQueue > 0 & CountPos < 9  ->
