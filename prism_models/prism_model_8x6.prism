// First vehicle on non-through lanes
module Vehicle8

    v8 : [0..7] init runningStatus;

    // Approaching transition
    [] !dummyFirst & v8 = runningStatus & nonThroughQueue < maxQueueSize -> (v8' = approachingStatus) & (nonThroughQueue' = nonThroughQueue + 1);

    // Immediate crossing if in prioritized mode and no vehicle is crossing
    [] v8 = approachingStatus & ((mode = 0 & CountPos+1 = dPos) | throughQueue = 0) & !isCrossing -> (v8' = crossingStatus) & (isCrossing' = true);
    [] v8 = approachingStatus & ( mode = 1 & turn = 1) & !isCrossing -> (v8' = crossingStatus) & (isCrossing' = true) & (turn' = 0);

    // Stopping transition
    [stopped] v8 = approachingStatus & isCrossing -> (v8' = stoppedStatus);

    // Entering transition
    [] v8 = stoppedStatus & mode = 0 & !isCrossing -> (v8' = crossingStatus) & (isCrossing' = true);
    [] v8 = stoppedStatus & ( mode = 1 & turn = 1 ) & !isCrossing -> (v8' = crossingStatus) & (turn' = 0) & (isCrossing' = true);

    // Exiting transition
    [] v8 = crossingStatus & nonThroughQueue > 0 -> (v8' = crossedStatus) & (isCrossing' = false) & (nonThroughQueue'=nonThroughQueue-1);

endmodule

